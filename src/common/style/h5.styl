.main-h5
    height 100%
    width 100%
    position relative
    background: url(../image/Grid-Teal@2x.webp) left top
    background-repeat: repeat-y
    background-color: #132029
    &.bg8-h5
        background: url(../image/background@3x.webp) no-repeat center 
        background-size: cover
        background-color: #132029
    .top-h5
        height 4.6rem
        position absolute
        top 0
        left 0
        right 0  
        padding 0 2.4rem
        z-index 100
        background-color: #fff
        display flex
        align-items: center
        justify-content: space-between
        .logo
            height: 4.6rem
            display flex
            align-items: center
            .logo-img
                height 2.6rem  
        .mobile-trigger
            .menuButton
                display block
                width: 15px
                height 10px
                position relative
                transition: all 0.75s cubic-bezier(0.19, 1, 0.22, 1)
                &:after,&:before
                    transition: all 0.75s cubic-bezier(0.19, 1, 0.22, 1)
                    content: ''
                    background-color: #009aa6
                    height 2px
                    width 15px
                    position absolute
                    left 0
                &:before
                    bottom 0
            .open
                transform: rotate(45deg) translate(2px, 0)
                &:after
                    transform: rotate(0) translate(0, -2px)
                    bottom 0
                &:before
                    transform: rotate(-90deg) translate(-6px, 0);
                    top 0         
    .bgImg
        position absolute
        right 0
        top 50%
        width: 70%
        transform: translate(0, -50%)
        z-index -1
    .goDown
        position absolute
        bottom 2.4rem
        left 50%
        transform: translate(-50%, -25%)
        z-index 1000
        transition: all 1s ease
        &.active
            transform: translate(-50%, 0)
    .menu-wrapper
        position absolute
        width 100%
        left 0
        top 4.6rem
        height calc(100vh - 4.6rem)
        background-color: #fff
        transition: all 0.7s cubic-bezier(0.19, 1, 0.22, 1)
        transform: translateY(-100%)
        z-index 1
        &.open
            transform: translateY(0) 
        ul
            padding 2.4rem 2.4rem 0
            .menu-item
                padding 0 1rem
                margin-bottom 3.5rem
                font-weight: 600
                color #009aa6
    .page1
        position relative      
        .content-h5
            position absolute
            padding 0 2.4rem
            left 0
            right 0
            top 50vh          
            transform: translate(0, -50%)
            .text-box
                color #f0f0f0            
                .title-primary
                    display inline-block
                    padding 0 1rem
                    line-height: 4rem
                    height 4rem
                    font-size: 2rem
                    margin-bottom 2rem
                    border-radius: 4px
                    background-color: #009aa6
                    font-weight: 600
                .title
                    margin-bottom 1rem
                    line-height: 4rem
                    font-size: 2.8rem                       
                .sub-title 
                    line-height: 2rem
                    font-size: 1.4rem
                    color #e5e5e5
    .page2
        .content-h5
            position absolute
            padding 0 2.4rem
            left 0
            right 0
            top 50vh
            transform: translate(0, -50%)
            .text-box
                .title-primary
                    display inline-block
                    padding 0 2rem
                    line-height: 5rem
                    height 5rem
                    font-size: 1.8rem
                    font-weight: 600
                    margin-top 4.6rem
                    border-radius: 4px
                    background-color: #009aa6
                    color #fff
                    cursor pointer
                    &:hover
                        background-color: #63ceca
                .title 
                    margin-bottom 1.6rem
                    line-height: 4rem
                    font-size: 2.8rem   
                    color #63ceca 
                .sub-title 
                    line-height: 2rem
                    font-size: 1.4rem
                    color #d0d4d3
    .page3,.page10
        .bg-cover
            position absolute
            top 0
            bottom 0
            left 0
            right 0
            background-color: rgba(0,0,0,0.4)
            z-index -1
        .content-h5
            position absolute
            padding 0 2.4rem
            left 0
            right 0
            top 50vh
            transform: translate(0, -50%)   
            .left
                .column
                    padding 1.8rem 0
                    z-index 1000
                    .title
                        margin-bottom: 1rem
                        font-size 2rem
                        color #63ceca
                        font-weight: 600
                    .sub-title
                        margin-bottom: 1rem
                        font-size 1.2rem
                        color #63ceca
                        span
                            display inline-block
                            padding 0 .4rem
                            margin-right 2rem
                            background-color: rgba(255, 255, 255, 0.1)
                    .list
                        color #d0d4d3
                        &.list-row
                            display flex
                            flex-wrap: wrap
                            li
                                margin-right 2.8rem
                                list-style-type square
                        li
                            list-style-type disc
                            font-size: 1.2rem   
                            line-height: 2rem          
    .page4,.page5,.page6,.page7
        .content-h5
            position absolute
            padding 0 2.4rem
            left 0
            right 0
            top 50vh
            transform: translate(0, -50%)  
            .tips
                margin-bottom 1rem
                color #d0d4d3
                font-size 1.6rem
                font-weight: 600
            .title
                margin-bottom: 5rem
                line-height: 3.6rem
                font-size 2.8rem
                color #63ceca
            .serve-container               
                margin-bottom: 2.4rem
                .serve-column
                    .image-block
                        margin-bottom 2rem
                        width 5rem
                        height 5rem
                        background-size: cover
                        background: url(../image/fill-1.svg) no-repeat center
                        &.block2
                            background-image: url(../image/fill-2.webp)
                        &.block3
                            background-image: url(../image/fill-3.webp)
                        &.block4
                            background-image: url(../image/fill-4.webp)
                    .column-title
                        margin-bottom 1.2rem
                        line-height: 2.4rem
                        font-size 2.2rem
                        color #63ceca
                    .column-text
                        line-height: 2rem
                        font-size: 1.4rem
                        color #d0d4d3
    .page8
        .content-h5
            position absolute
            padding 0 2.4rem
            left 0
            right 0
            top 50vh
            transform: translate(0, -50%)
            .tips
                margin-bottom 1rem
                color #d0d4d3
                font-size 1.6rem
                font-weight: 600
                text-align: center
            .title         
                margin-bottom 4rem
                line-height: 4rem
                font-size: 2.8rem 
                color #63ceca
                text-align: center
            .card-swipe
                overflow hidden
                .card-slide
                    width 9999px
                    .card-list
                        float left
                        color #fff                
                        width: calc((100vw - 4.8rem) / 2)                
                        transform: translate(0, 0)
                        transition: transform 0s
                        &.active
                            transform: translate(-100%, 0)
                            transition: transform 1s ease
                        .image-box
                            box-sizing: content-box
                            width: 80%
                            margin-left 10%
                            padding-top 80%
                            margin-bottom 20%
                            background-color: #fff
                            font-size: 0
                            background-repeat: no-repeat
                            background-size: cover
    .page9,.page10
        .content-h5
            position absolute
            padding 0 2.4rem
            left 0
            right 0
            top 50vh
            transform: translate(0, -50%)            
            .title
                margin-top 4rem
                margin-bottom 2rem
                line-height: 4rem
                font-size: 2.8rem 
                color #63ceca
                text-align: center
            .container
                overflow hidden
                position relative
                width: 100%     
                .slide-box
                    overflow hidden
                    position relative
                    width 9999px
                    left 0
                    transition: all 1s ease
                    .serve-box                    
                        float left                     
                        width calc(100vw - 4.8rem) 
                        padding 0 0rem
                        box-sizing: border-box
                        .image-box                           
                            img 
                                width 100%
                                height 75%
                            .info
                                margin-top 1.5rem
                                color #f0f0f0
                                p
                                    font-size: 1.2rem   
                                    margin 0.4rem 0
                        .tips            
                            margin-bottom 2rem
                            line-height: 2rem
                            font-size: 1.4rem           
                            color #009aa6  
        .footer
            position fixed
            bottom 0
            left 0
            right 0
            height 5rem
            padding 2.5rem 0
            background: url(../image/footer-banner.png) no-repeat center
            background-size: cover
            background-color: rgba(0, 0, 0, .5) 
            transform: translate(-100%, 0)
            opacity 0
            transition: all 1s .5s ease
            &.footer-show
                opacity 1     
                transform: translate(0, 0) 
            .logo-img
                margin 0 auto
                height 2.5rem
                display block
            .email
                margin-top 1rem
                font-size: 1.4rem
                color #63ceca
                text-align: center   
                span
                    margin-right 2rem 
            .close
                position absolute
                right 2.4rem
                top 1rem
                height 2.2rem
                line-height: 2.2rem
                padding 0 .4rem
                color #999
                border-radius: 4px
                font-size 1.4rem
                cursor pointer
                &:hover
                    color #e64360
    .page10
        .email
            margin-bottom 4rem
            font-size: 1.6rem
            color #f0f0f0
            text-align: center   
            span
                margin-right 1.4rem 
